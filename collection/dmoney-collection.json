					{
						"key": "Authorization",
						"value": "{{token}}",
						"type": "text"
					},
					{
						"key": "X-AUTH-SECRET-KEY",
						"value": "ROADTOSDET",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\":\"{{$randomFirstName}}\",\n    \"email\":\"{{$randomEmail}}\",\n    \"password\":\"{{$randomPassword}}\",\n    \"phone_number\":\"0152{{phoneNumber}}\",\n    \"nid\":\"123456789\",\n    \"role\":\"Customer\"\n}\n",
					"options": {
